{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "server",
    "generateOptions": {
        "flat": false,
        "spec": false
    },
    "compilerOptions": {
        "deleteOutDir": true,
        "assets": [{ "include": "../env/*", "outDir": "./dist/env" }]
    }
}
